  {
    "_id": "5d5d36b061276e3b4b79d236",
    "auteur": "kid_Bytb5ETpE",
    "dateDeCreation": "2019-05-30T11:09:57.266Z",
    "dechetId": "5d01efcd2138b22a95a2419a",
    "localisationSolution": "lyon",
    "nom": "DIY: pochette en emballage plastique",
    "type": "revalorisation_UpcyclingTuto",
    "valideParAdmin": 1,
    "content": {
      "revalorisation_UpcyclingTuto_source": "www.planetaddict.com/upcycling-plastique/",
      "revalorisation_UpcyclingTuto_materielList": [
        "Un emballage alimentaire propre",
        "Une fermeture éclaire (de taille plus grande que l’emballage)",
        "Une feuille de plastique collant de type protège cahier",
        "Une machine à coudre",
        "Des ciseaux",
        "Une règle"
      ],
      "revalorisation_UpcyclingTuto_actions": [
        {
          "index": 0,
          "text": "Découper les bords haut et bas de l’emballage et choisir le sens pour la pochette, puis ouvrir les côtés. On peut aussi choisir d’assembler deux emballages."
        },
        {
          "index": 1,
          "text": "Coller le film plastique transparent de chaque côté, en s’aidant d’une règle pour éviter les bulles d’air."
        },
        {
          "index": 2,
          "text": "Placer la fermeture éclair ouverte sur l’endroit de l’emballage (le côté qui se voit), côté fermeture sur le bord de l’emballage et les « dents » vers le bas."
        },
        {
          "index": 3,
          "text": "Coudre en prenant soin de rester droit et de ne pas rompre l’emballage."
        },
        {
          "index": 4,
          "text": "Plier sur la couture pour rabattre la fermeture vers l’intérieur et coudre une seconde fois."
        },
        {
          "index": 5,
          "text": "Procéder de même pour l’autre face de la pochette. C’est un peu plus difficile étant donné que l’autre côté de la fermeture est déjà cousu."
        },
        {
          "index": 6,
          "text": "Coudre les bords de la pochette (et le bas si ce n’est pas sur un plus), sur l’envers. Insister sur les coins en passant plusieurs fois."
        },
        {
          "index": 7,
          "text": "Retourner délicatement la pochette pour ne pas déchirer les coutures et travailler les coins."
        }
      ]
    },
